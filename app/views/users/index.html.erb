<div class="panel panel-default">
  <div class="panel-heading">
    <h1>Listing users</h1>
  </div>
  <div class="panel-body">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>Phone</th>
          <th>Page</th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.full_name, user %></td>
            <td><%= user.phone %>
            <td><%= link_to 'Page', user_send_page_path(user), class: "btn btn-default", method: :post %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
   </div>
</div>